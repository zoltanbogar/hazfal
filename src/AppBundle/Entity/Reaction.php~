     * Set reactionType.
     *
     * @param int $reactionType
     *
     * @return Reaction
     */
    public function setReactionType($reactionType)
    {
        $this->reactionType = $reactionType;

        return $this;
    }

    /**
     * Get reactionType.
     *
     * @return int
     */
    public function getReactionType()
    {
        return $this->reactionType;
    }
